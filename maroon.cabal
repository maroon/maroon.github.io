name:               maroon
version:            0.1.0.0
build-type:         Simple
cabal-version:      >= 1.10

executable site
  main-is:            site.hs
  ghc-options:        -Wall
  ghc-options:        -threaded
  default-language:   Haskell2010
  build-depends:
      base == 4.11.*
    , hakyll == 4.12.*
    , filepath == 1.4.*
    , yaml == 0.8.*
